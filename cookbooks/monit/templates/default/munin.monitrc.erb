  check process munin-node with pidfile /var/run/munin/munin-node.pid
    start program = "/etc/init.d/munin-node start" with timeout 60 seconds
    stop program  = "/etc/init.d/munin-node stop"
    if cpu > 60% for 2 cycles then alert
#    if cpu > 80% for 5 cycles then restart
    if totalmem > 20.0 MB for 5 cycles then alert
    if children > 10 then alert
    if failed host 127.0.0.1 port 4949 with timeout 15 seconds then alert
    if 3 restarts within 5 cycles then timeout
    group server

